<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < category.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= category[i].name %></td>
          <td>
            <form action="/category/delete/<%= category[i]._id %>?_method=DELETE" method="POST">
              <a href="/category/edit/<%= category[i]._id %>" class="btn btn-primary">
                <i class="fas fa-pencil-alt"></i> Ubah</a>
              <button class="btn btn-danger"><i class="fas fa-trash-alt"></i> Hapus</button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>